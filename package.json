      "@nestjs/core": "^7.0.0",
      "@nestjs/jwt": "^7.1.0",
      "@nestjs/passport": "^7.1.0",
      "@nestjs/platform-express": "^7.0.0",
      "@nestjs/serve-static": "^2.1.3",
      "@nestjs/typeorm": "^7.1.4",
      "bcrypt": "^5.0.0",
      "class-validator": "^0.12.2",
      "cloudinary": "^1.23.0",
      "dotenv": "^8.2.0",
      "firebase": "^7.23.0",
      "jsonwebtoken": "^8.5.1",
      "multer": "^1.4.2",
      "mysql": "^2.14.1",
      "passport": "^0.4.1",
      "passport-jwt": "^4.0.0",
      "reflect-metadata": "^0.1.10",
